#
alias "c=cd"
alias ".."="cd .."
alias "..."="cd ../.."
alias "ll"="ls -lah"
alias "ls=ls -h"

# git
alias g=git
alias gs="git status"
alias gc="git commit -m"
alias ga="git add ."
alias gd="git diff"
alias gl="git log"

# docker
alias dcleanup="docker system prune -f"
alias di="docker images"

# feh
alias feh="feh -B white"
